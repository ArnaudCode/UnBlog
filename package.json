{
  "name": "ArnaudFaitUnBlog",
  "description": "Arnaud fait un blog",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ArnaudFait/UnBlog.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "meteor run --settings settings-dev.json",
    "lint": "eslint --ext .js,.jsx .",
    "upgrade": "meteor npm update && meteor update --all-packages",
    "reset": "meteor reset && meteor npm prune && meteor npm start",
    "fresh": "meteor npm run upgrade && meteor npm run reset",
    "ios": "meteor run ios --settings settings-dev.json",
    "ios-device": "meteor run ios-device --settings settings-dev.json",
    "android": "meteor run android --settings settings-dev.json",
    "deploy": "DEPLOY_HOSTNAME=eu-west-1.galaxy.meteor.com meteor deploy www.madl3ss.com --settings settings.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "bcrypt": "^5.0.0",
    "jquery": "^3.5.1",
    "meteor-node-stubs": "^1.0.1",
    "normalize.css": "^8.0.1",
    "simpl-schema": "^1.10.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.coffee",
      "server": "server/main.coffee"
    },
    "testModule": "tests/main.coffee"
  }
}
